@model IEnumerable<CarCleanz.Models.Booking>

<h2>Booking Details</h2>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Customer Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Vehicle Type</th>
            <th>Service Type</th>
            <th>Booking Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var booking in Model)
        {
            <tr>
                <td>@booking.Name</td>
                <td>@booking.Email</td>
                <td>@booking.Phone</td>
                <td>@booking.VehicleType</td>
                <td>@booking.ServicePackage</td>
                <td>@booking.BookingDate</td>
            </tr>
        }
    </tbody>
</table>