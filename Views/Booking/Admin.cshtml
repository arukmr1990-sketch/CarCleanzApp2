@model IEnumerable<CarCleanz.Models.Booking>

<h2>Booking Records (Admin Panel)</h2>

<table border="1" cellpadding="6" cellspacing="0">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Vehicle Type</th>
            <th>Service Package</th>
            <th>Booking Date</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var booking in Model)
    {
        <tr>
            <td>@booking.Name</td>
            <td>@booking.Email</td>
            <td>@booking.Phone</td>
            <td>@booking.VehicleType</td>
            <td>@booking.ServicePackage</td>
            <td>@booking.BookingDate</td>
        </tr>
    }
    </tbody>
</table>